<%= form_for([@event_type]) do |f| %>
  <fieldset>
    <div class="form-group">
      <div class="field">
        <%= f.label :name, class: "col-lg-2 control-label" %>
        <div class = "col-lg-10">
          <%= f.text_field :name, class: "form-control" %>
        </div>
      </div>
    </div>
    <div class="form-group">
      <div class="field">
        <%= f.label :event_type, class: "col-lg-2 control-label" %>
        <div class = "col-lg-10">
          <%= f.text_field :event_type, class: "form-control" %>
        </div>
      </div>
    </div>
    <div class="form-group">
      <div class="field">
        <%= f.label :sub_type, class: "col-lg-2 control-label" %>
        <div class = "col-lg-10">
          <%= f.text_field :sub_type, class: "form-control" %>
        </div>
      </div>
    </div>
    <div class="form-group">
      <div class="field">
        <%= f.label :type_url, class: "col-lg-2 control-label" %>
        <div class = "col-lg-10">
          <%= f.text_field :type_url, class: "form-control" %>
        </div>
      </div>
    </div>
    <div class="form-group">
      <div class="col-lg-10 col-lg-offset-2">
        <%= link_to 'Cancel', admin_event_types_path, class: "btn btn-default" %>
        <%= f.submit class: "btn btn-primary submit" %>
      </div>
    </div>
  </fieldset>
<% end %>
